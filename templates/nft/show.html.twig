{% extends 'base.html.twig' %}

{% block title %}Nft{% endblock %}

{% block body %}
    <h1 class="my-3 ms-3"><a href="{{ path('app_nft_index') }}" class="button display-5"><i class="fa-solid fa-arrow-left-long"></i></a>NFT<span class="float-end me-3 d-flex flex-row"><a href="{{ path('app_nft_edit', {'id': nft.id}) }}" class="button"><i class="fa-solid fa-pen"></i></a>{{ include('nft/_delete_form.html.twig') }}</span> </h1>

    <table class="table table-hover table-striped align-middle table-dark">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ nft.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ nft.name }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ nft.image }}</td>
            </tr>
            <tr>
                <th>DateDrop</th>
                <td>{{ nft.dateDrop ? nft.dateDrop|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>AnneeAlbum</th>
                <td>{{ nft.anneeAlbum }}</td>
            </tr>
            <tr>
                <th>IdentificationToken</th>
                <td>{{ nft.identificationToken }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ nft.slug }}</td>
            </tr>
        </tbody>
    </table>






{% endblock %}
